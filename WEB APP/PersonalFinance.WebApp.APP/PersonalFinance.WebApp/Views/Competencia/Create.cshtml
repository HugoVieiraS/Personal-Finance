@model PersonalFinance.WebApp.Model.Competencia

@{
    ViewData["Title"] = "Create";
}

<h1>Abrir competencia</h1>
<hr />
    <div class="col-md-4">
        <form asp-action="Create" method="post" role="form">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="card-body">
                <div class="form-group">
                    <label asp-for="DataInicial"></label>
                    <input asp-for="DataInicial" class="form-control" id="exampleInputEmail1" placeholder="Enter email">
                </div>
                <div class="form-group">
                    <label asp-for="DataFinal"></label>
                    <input asp-for="DataFinal" class="form-control" id="exampleInputPassword1" placeholder="Password">
                </div>
                <div class="card-footer">
                    <button type="submit" class="btn btn-primary">Confirmar</button>
                </div>
            </div>
        </form>
    </div>

<div>
    <a asp-action="Index">Voltar</a>
</div>

